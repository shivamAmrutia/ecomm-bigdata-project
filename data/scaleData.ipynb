    "import pandas as pd\n",
    "\n",
    "# Load dataset\n",
    "df = pd.read_csv(\"./2019-Nov.csv\")\n",
    "\n",
    "# Randomly sample ~20% of the data (since 1 GB is 1/5th of 5 GB)\n",
    "sampled_df = df.sample(frac=0.12, random_state=42)\n",
    "\n",
    "# Save new dataset\n",
    "sampled_df.to_csv(\"./scaledData-2019-Nov.csv\", index=False)\n"
   ]
  }
